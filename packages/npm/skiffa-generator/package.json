{
  "name": "skiffa-generator",
  "version": "0.11.0",
  "sideEffects": false,
  "description": "",
  "type": "module",
  "main": "./bundled/main.cjs",
  "module": "./bundled/main.js",
  "types": "./typed/main.d.ts",
  "exports": {
    ".": {
      "require": "./bundled/main.cjs",
      "import": "./bundled/main.js",
      "types": "./typed/main.d.ts"
    }
  },
  "files": [
    "./typed/**",
    "./bundled/**"
  ],
  "bin": {
    "skiffa-generator": "bundled/program.js"
  },
  "scripts": {
    "prepack": "node ./scripts/build.js",
    "pretest": "tsc",
    "build": "node ./scripts/build.js",
    "clean": "node ./scripts/clean.js",
    "test": "node --test ./transpiled/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/Skiffa"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/Skiffa/issues"
  },
  "keywords": [
    "openapi",
    "generator"
  ],
  "dependencies": {
    "@jns42/core": "^0.7.0",
    "@jns42/oas-v3-0": "^0.17.4",
    "@jns42/oas-v3-1": "^0.17.4",
    "@jns42/swagger-v2": "^0.17.4",
    "@skiffa/core": "^0.2.2",
    "@types/node": "^18.19.39",
    "@types/yargs": "^17.0.32",
    "camelcase": "^8.0.0",
    "goodrouter": "^2.1.6",
    "jns42-generator": "^0.21.0",
    "skiffa-lib": "^0.10.6",
    "tslib": "^2.6.3",
    "type-fest": "^4.20.1",
    "yaml": "^2.4.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.7",
    "@tsconfig/node18": "^18.2.4",
    "rollup": "^4.18.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=18"
  }
}
