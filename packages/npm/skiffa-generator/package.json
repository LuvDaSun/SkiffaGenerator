    "openapi",
    "generator"
  ],
  "dependencies": {
    "@jns42/core": "^0.7.11",
    "@jns42/generator": "^0.21.16",
    "@skiffa/core": "^0.2.8",
    "@skiffa/lib": "^0.13.0",
    "@types/node": "^22.15.3",
